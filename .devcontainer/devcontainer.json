{
    "name": "third_wheeler_dev",
    "dockerFile": "../DockerFile",
    "build": { "target": "dev" },
    "workspaceFolder": "/app",
    "runArgs": [
        "--name=third_wheeler",
        "--network=host"
    ],
    "customizations": {
        "settings": {
            "terminal.integrated.shell.linux": "/bin/bash"
        },
        "extensions": []
    },
    "workspaceMount": "src=${localWorkspaceFolder},dst=/app,type=bind,consistency=cached",
    "postCreateCommand": ""
}
